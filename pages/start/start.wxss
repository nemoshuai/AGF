page{
  height: 100%;
}

.start{
  width: 100%;
  height: 100%;  
  -webkit-animation: welcome 3s ease-in-out infinite;
  animation: welcome 3s ease-in-out infinite; 
  
}

@keyframes welcome{
  0%{
    opacity: 0
  }
  25%{
    opacity: 0.5
  }
  50%{
    opacity: 1
  }
  100%{
    opacity: 0.2
  }
}